{
  "name": "samsonphp/resource",
  "description": "SamsonPHP static resources management module",
  "keywords": [
    "resource",
    "static"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    },
    {
      "name": "Kotenko Nikita",
      "email": "kotenko@samsonos.com"
    }
  ],
  "require": {
    "samsonframework/localfilemanager": "1.*",
    "samsonos/php_core": "*",
    "samsonphp/event": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "replace": {
    "samsonos/php_resourcer": "*"
  },
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "samsonphp\\resource\\": "src/"
    },
    "files": [
      "src/global.php"
    ]
  },
  "config": {
    "github-protocols": [
      "https"
    ]
  }
}
